{
  "name": "@giof/react-umami",
  "version": "2.2.0",
  "description": "Umami Analytics component for React applications with dry-run testing and debug features",
  "keywords": [
    "umami",
    "analytics",
    "react",
    "umami-react",
    "react-umami"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/giof-se/react-umami.git"
  },
  "homepage": "https://github.com/giof-se/react-umami#readme",
  "bugs": {
    "url": "https://github.com/giof-se/react-umami/issues"
  },
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "build": "tsup src/index.tsx --format esm,cjs --dts",
    "fmt": "biome format --write .",
    "lint": "biome check src tests *.json",
    "lint:write": "biome check --write src tests *.json",
    "prepare-release": "pnpm test && pnpm build && changelogen --output CHANGELOG.md --release",
    "prepublishOnly": "pnpm build",
    "release": "pnpm validate && bumpp package.json --commit \"release: v%s\" --tag \"v%s\" && git push --follow-tags",
    "test": "vitest run",
    "test:coverage": "vitest run --coverage",
    "test:watch": "vitest",
    "typecheck": "tsc --noEmit",
    "validate": "pnpm lint && pnpm typecheck && pnpm test",
    "changelog": "changelogen --output CHANGELOG.md",
    "changelog:release": "changelogen --output CHANGELOG.md --release",
    "version:patch": "bumpp package.json --commit \"release: v%s\" --tag \"v%s\" --push --patch",
    "version:minor": "bumpp package.json --commit \"release: v%s\" --tag \"v%s\" --push --minor",
    "version:major": "bumpp package.json --commit \"release: v%s\" --tag \"v%s\" --push --major",
    "prepare": "husky"
  },
  "devDependencies": {
    "@biomejs/biome": "2.0.5",
    "@testing-library/react": "^16.3.0",
    "@total-typescript/tsconfig": "^1.0.4",
    "@types/node": "^24.0.4",
    "@types/react": "^19.1.8",
    "@vitejs/plugin-react": "^4.6.0",
    "@vitest/coverage-v8": "^3.2.4",
    "bumpp": "^10.2.0",
    "changelogen": "^0.6.1",
    "happy-dom": "^18.0.1",
    "husky": "^9.1.7",
    "jsdom": "^26.1.0",
    "lint-staged": "^16.1.2",
    "tsup": "^8.5.0",
    "typescript": "^5.8.3",
    "vitest": "^3.2.4"
  },
  "peerDependencies": {
    "react": "^18.0.0 || ^19.0.0"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org"
  },
  "pnpm": {
    "ignoredBuiltDependencies": [
      "esbuild"
    ],
    "onlyBuiltDependencies": [
      "@biomejs/biome"
    ]
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "biome format --write"
    ],
    "{src,tests}/**/*.{js,jsx,ts,tsx}": [
      "biome check --write"
    ],
    "*.{json,md}": [
      "biome format --write"
    ]
  }
}